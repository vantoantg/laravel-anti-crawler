{
    "name": "vantoantg/laravel-anti-crawler",
    "description": "Comprehensive anti-crawler and bot protection for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "anti-crawler", "bot-detection", "rate-limiting", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Toan Nguyen",
            "email": "nguyennguyen.vt88@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/redis": "^11.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VanToanTG\\AntiCrawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanToanTG\\AntiCrawler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VanToanTG\\AntiCrawler\\AntiCrawlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
